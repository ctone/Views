@model IList<DiethelmTSWebsite2015.Models.SysCategory>
@{
    Layout = null;
}

<div class="row" > @*style="margin-top: 22px;"*@
    <div class="medium-12 large-12 column" id="title-bar">
        <h5>....</h5>
    </div>
</div>
<div class="row side-menu">
