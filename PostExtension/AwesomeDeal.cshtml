@model IList<DiethelmTSWebsite2015.Models.SysPost>
@{
    Layout = null;
}

<div class="row" id="AwesomeDeal">
    <div class="small-12 medium-12 columns">
        @if(Model.Count()>0)
        {
            <h4 class="content-subject">Awesome Deals</h4>
        }
    </div>
    <div class="small-12 medium-12 columns content-detail">
        <div class="row">
            @foreach (var item in Model)
            {
                @Html.DisplayFor(modelItem => item, "ContentVerticalLarge");
         
                //  @Html.Partial("~/Views/Shared/DisplayTemplates/CorporateContentItemByCategory.cshtml", item)
            }
        </div>
    </div>
</div>
